@section scripts{
    <script src="~/js/ScriptOrder.js"></script>
    <script type="text/javascript">
        $(function () {
              $.ajaxSetup({ cashe: false });
              $('.ord').click(function (e) {
                  e.preventDefault();
                  $.get(this.href, function (data) {
                      $("#dialogContent").html(data);
                      $("#modDialog").modal("show");
                  });
              });

          })

    </script>
