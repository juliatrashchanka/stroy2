@model Order

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<div class="modal-content">
    <div class="modal-header">
        <button class="close" data-bs-dismiss="modal" area-hidden="true">X</button>
        <h4>О заказе</h4>
    </div>
    <div class="modal-body">
        <dl class="dl-horizontal">

            <dt>Наименование работы:</dt>
            <dd>@Html.DisplayFor(model => model.Work)</dd>

            <dt>Объём работ:</dt>
            <dd>@Html.DisplayFor(model => model.Volume)</dd>

            <dt>Населенный пункт</dt>
            <dd>@Html.DisplayFor(model => model.Locality)</dd>

            <dt>Материалы заказчика или подрядчика?</dt>
            <dd>@Html.DisplayFor(model => model.Material)</dd>
        </dl>
    </div>
</div>